{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react-jsx",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@components/*":["./src/components/*"],
      "@libs/*":["./src/libs/*"],
    }
  },
  "include": [
    "src"
  ],
}